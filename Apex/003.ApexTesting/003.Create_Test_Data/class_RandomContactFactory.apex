public class RandomContactFactory {    public static List<Contact> generateRandomContacts(Integer noOfCon, String sLastName){        List<Contact> sContact = new List<Contact>();        try{            if (noOfCon == null){                ExceptionError.Error('No of Contacts is null', 'RandomContactFactory', 'generateRandomContacts');                            }            else if (noOfCon < 1){                ExceptionError.Error('No of Contacts less than 1', 'RandomContactFactory', 'generateRandomContacts');            }            else if (noOfCon > 199){                ExceptionError.Error('No of Contacts greater than 200', 'RandomContactFactory', 'generateRandomContacts');                            }            else if (sLastName == null){                ExceptionError.Error('Last Name input is null', 'RandomContactFactory', 'generateRandomContacts');                            }                        String sTimeStamp;                        for (Integer i = 0; i<noOfCon; i++){                sTimeStamp = 'Jaba_'+i+'_'+(String)(System.Datetime.now().format('YMDHms_S'));                sContact.add(new Contact(FirstName = sTimeStamp, LastName = sLastName));            }            System.debug('Contact List Size =' + sContact.size());        }           catch(Exception e){			ExceptionError.Errore(e, 'RandomContactFactory', 'generateRandomContacts');                    }            return sContact;    }}